<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.syiyi.com.ipcdemp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".launcher.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ibinder.base.BaseIbinderAC" />
        <activity android:name=".ibinder.advanced.AdvancedIbinderAC" />
        <activity android:name=".ibinder.google.GIbinderAC" />

        <service
            android:name=".ibinder.base.IbinderService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".ibinder.advanced.AdvancedService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".ibinder.google.GService"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>
